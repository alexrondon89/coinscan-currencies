{
  "service": "coinscan-currencies",
  "version": "0.0.1",
  "port": ":4000",
  "coingecko":{
    "url": {
      "baseUrl": "api.coingecko.com",
      "endpoints": {
        "ping":"/ping",
        "coinInfo":"/api/v3/coins/:coinid"
      }
    }
  },
  "cache": {
    "timeToUpdate": 40,
    "numberOfElements": 500,
    "maxNumberOfElements": 1000
  }
}